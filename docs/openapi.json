{
  "openapi": "3.1.0",
  "info": {
    "title": "APWorld Package Index",
    "description": "Repository server for APWorld package hosting and discovery",
    "version": "0.1.0"
  },
  "paths": {
    "/v1/packages": {
      "get": {
        "tags": [
          "packages"
        ],
        "summary": "List Packages",
        "description": "List all packages with pagination.\n\nReturns a paginated list of all packages in the repository,\nsorted by name alphabetically.",
        "operationId": "list_packages_v1_packages_get",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 1,
              "description": "Page number",
              "default": 1,
              "title": "Page"
            },
            "description": "Page number"
          },
          {
            "name": "per_page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1,
              "description": "Items per page",
              "default": 20,
              "title": "Per Page"
            },
            "description": "Items per page"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PackageListResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/search": {
      "get": {
        "tags": [
          "packages"
        ],
        "summary": "Search Packages",
        "description": "Search packages by name, keyword, author, or game.\n\nSupports filtering by:\n- q: Free text search across name, display_name, description, and keywords\n- game: Filter by game name (from version metadata)\n- author: Filter by author name\n- compatible_with: Filter by Core AP version compatibility",
        "operationId": "search_packages_v1_search_get",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "description": "Search query",
              "default": "",
              "title": "Q"
            },
            "description": "Search query"
          },
          {
            "name": "game",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Filter by game name",
              "title": "Game"
            },
            "description": "Filter by game name"
          },
          {
            "name": "author",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Filter by author name",
              "title": "Author"
            },
            "description": "Filter by author name"
          },
          {
            "name": "compatible_with",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Filter by Core AP version compatibility",
              "title": "Compatible With"
            },
            "description": "Filter by Core AP version compatibility"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 1,
              "description": "Page number",
              "default": 1,
              "title": "Page"
            },
            "description": "Page number"
          },
          {
            "name": "per_page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1,
              "description": "Items per page",
              "default": 20,
              "title": "Per Page"
            },
            "description": "Items per page"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SearchResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/packages/{name}": {
      "get": {
        "tags": [
          "packages"
        ],
        "summary": "Get Package",
        "description": "Get full metadata for a specific package.\n\nReturns detailed package information including all versions,\nauthors, keywords, and download statistics.",
        "operationId": "get_package_v1_packages__name__get",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Name"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PackageMetadata"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/packages/{name}/versions": {
      "get": {
        "tags": [
          "packages"
        ],
        "summary": "List Versions",
        "description": "List all versions of a package.\n\nReturns all versions sorted by publication date (newest first).\nBy default, yanked versions are excluded.",
        "operationId": "list_versions_v1_packages__name__versions_get",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Name"
            }
          },
          {
            "name": "include_yanked",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "description": "Include yanked versions",
              "default": false,
              "title": "Include Yanked"
            },
            "description": "Include yanked versions"
