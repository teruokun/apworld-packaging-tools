    "/v1/index.json": {
      "get": {
        "tags": [
          "packages"
        ],
        "summary": "Get Index",
        "description": "Get full package index for offline tooling.\n\nReturns a complete index of all packages and versions,\nsuitable for offline package resolution and caching.",
