        "summary": "Get Version",
        "description": "Get metadata for a specific version.\n\nReturns detailed version information including all distributions\nand their download URLs.",
        "operationId": "get_version_v1_packages__name___version__get",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Name"
            }
          },
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Version"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VersionMetadata"
                }
              }
